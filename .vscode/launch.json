{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "cmake",
            "request": "launch",
            "name": "CMake: Configure project",
            "cmakeDebugType": "configure",
            "clean": false,
            "configureAll": false
        },
        {
            "type": "cmake",
            "request": "launch",
            "name": "CMake: Build project",
            "cmakeDebugType": "build",
            "clean": false,
            "configureAll": false
        },
        {
            "type": "cmake",
            "request": "launch",
            "name": "CMake: Run project",
            "cmakeDebugType": "run",
            "clean": false,
            "configureAll": false
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "program": "${workspaceFolder}/app.js",
            "outFiles": [
                "${workspaceFolder}/out/**/*.js"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "Attach",
            "port": 9229,
            "request": "attach",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node"
        },
        {
            "args": [
                "--extensionDevelopmentPath=${workspaceFolder}"
            ],
            "name": "Launch Extension",
            "outFiles": [
                "${workspaceFolder}/out/**/*.js"
            ],
            "preLaunchTask": "npm",
            "request": "launch",
            "type": "extensionHost"
        },
        {
            "name": "Run ESLint",
            "type": "node",
            "request": "launch",
            "runtimeExecutable": "eslint",
            "cwd": "${workspaceFolder}",
            "args": [
                "--ext",
                ".js,.jsx,.ts,.tsx",
                "."
            ]
        },
        {
            "name": "Run Prettier",
            "type": "node",
            "request": "launch",
            "runtimeExecutable": "prettier",
            "cwd": "${workspaceFolder}",
            "args": [
                "--write",
                "**/*.{js,jsx,ts,tsx,json,css,scss,html}"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Run autoprefixer",
            "runtimeExecutable": "autoprefixer",
            "cwd": "${workspaceFolder}",
            "args": []
        },
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "type": "chrome",
            "request": "launch",
            "name": "Launch Chrome against localhost",
            "url": "http://localhost:8080",
            "webRoot": "${workspaceFolder}",
            "sourceMapPathOverrides": {
                "webpack:///./*": "${webRoot}/*",
                "webpack:///src/*": "${webRoot}/src/*"
            }
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Node.js",
            "program": "${workspaceFolder}/server.js",
            "console": "integratedTerminal"
        }
    ]
]
// Add your configurations here.